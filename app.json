{
	"expo": {
		"name": "Daily Tasks",
		"description": "In this app you can configure and manage your tasks for a specific day and also add repetition for a specific period of time, so you won't forget about your duties and get the job done.",
		"slug": "daily-tasks",
		"privacy": "public",
		"platforms": ["ios", "android"],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#101010"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"userInterfaceStyle": "dark",
		"ios": {
			"bundleIdentifier": "pl.samuelk.daily-tasks",
			"buildNumber": "1.0.1",
			"supportsTablet": true
		},
		"android": {
			"package": "pl.samuelk.daily_tasks",
			"versionCode": 2,
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive.png",
				"backgroundColor": "#101010"
			},
			"permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
		},
		"androidNavigationBar": {
			"visible": "immersive",
			"barStyle": "light-content",
			"backgroundColor": "#101010"
		},
		"androidStatusBar": {
			"hidden": false,
			"translucent": true,
			"barStyle": "light-content",
			"backgroundColor": "#101010"
		}
	}
}
